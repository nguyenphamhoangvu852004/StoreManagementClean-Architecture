package com.example.dtos.productInfoDetail;

public class DienMayDetailInfoDTO extends ProductHangHoaDetailInfoDTO {

    private int thoiGianBaoHanh; // Số tháng
    private double congSuat; //

    public DienMayDetailInfoDTO(String maHang, String tenHang, int soLuongTon, double donGia, String tenLoai) {
        super(maHang, tenHang, soLuongTon, donGia, tenLoai);
    }

    public int getThoiGianBaoHanh() {
        return thoiGianBaoHanh;
    }

    public double getCongSuat() {
        return congSuat;
    }
}
