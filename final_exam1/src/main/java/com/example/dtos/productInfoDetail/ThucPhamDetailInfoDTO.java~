package com.example.dtos.productInfoDetail;

import java.time.LocalDate;

public class ThucPhamDetailInfoDTO extends ProductHangHoaDetailInfoDTO {

    private LocalDate ngaySanXuat;
    private LocalDate ngayHetHan;
    private String nhaCungCap;

    public ThucPhamDetailInfoDTO(String maHang, String tenHang, int soLuongTon, double donGia, String tenLoai) {
        super(maHang, tenHang, soLuongTon, donGia, tenLoai);
    }


    public LocalDate getNgaySanXuat() {
        return ngaySanXuat;
    }

    public LocalDate getNgayHetHan() {
        return ngayHetHan;
    }

    public String getNhaCungCap() {
        return nhaCungCap;
    }
}
